<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.lawyerassistance"
    tools:targetApi="m">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.LawyerAssistance"
        android:usesCleartextTraffic="true">
        <activity android:name=".login"/>
        <activity android:name=".Dashboard" />
        <activity android:name=".Lawyer_signup" />
        <activity android:name=".Exit_dialoge" />
        <activity android:name=".User_choice" />
        <activity android:name=".splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Client_signup" />
    </application>

</manifest>